{
    "name": "coisa/factory",
    "description": "Object Factory Utility",
    "type": "library",
    "require": {
        "php": ">=5.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Say√£o Lobato Abreu",
            "email": "github@felipeabreu.com.br"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "coisa/php-cs-fixer": "^1.0",
        "jangregor/phpstan-prophecy": "^0.6.2",
        "malukenho/docheader": "^0.1.8",
        "phpstan/phpstan": "^0.12.18",
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "CoiSA\\Factory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoiSA\\Factory\\": "tests"
        }
    },
    "scripts": {
        "analyze": "phpstan analyze",
        "check": [
            "@test",
            "@sniff",
            "@cs-check",
            "@analyze",
            "@docheader-check"
        ],
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix",
        "docheader-check": "docheader check ./src ./tests",
        "sniff": "phpcs",
        "test": "phpunit --color=always"
    }
}
